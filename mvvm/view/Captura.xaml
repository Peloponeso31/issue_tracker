                                    HorizontalAlignment="Right" 
                                    Margin="0,205,222,0" 
                                    VerticalAlignment="Top" 
                                    Width="125" Height="34" 
                                    RenderTransformOrigin="0,0.5" 
                                    Background="{StaticResource CustomColorg}" 
                                    Click="Button_Click"/>

                            <Button Style="{DynamicResource ButtonStyle1}" 
                                    Content="Borrar" 
                                    HorizontalAlignment="Right" 
                                    Margin="0,258,222,0" 
                                    VerticalAlignment="Top" 
                                    Width="125" Height="34" 
                                    Background="{StaticResource CustomColorr}"/>

                            <DataGrid x:Name="dataGridView1" 
                                      Margin="10,324,74,32" 
                                      AutoGenerateColumns="False">
