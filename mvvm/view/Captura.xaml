                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>

                                    <DataGridTextColumn Header="Coordenadas" Binding="{Binding Coordenadas}"/>

                                </DataGrid.Columns>
                            </DataGrid>
                        </Grid>
                    </TabItem>
                </TabControl>

            </Border>
        </Grid>
    </ScrollViewer>
</UserControl>
