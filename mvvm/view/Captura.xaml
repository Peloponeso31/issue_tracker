                                            <TextBox Margin="10,0,0,0" MinWidth="150"  ></TextBox>
                                        </StackPanel>
                                        <StackPanel>
                                            <TextBlock TextWrapping="Wrap" Style="{DynamicResource TabItemContentTextStyle }" Margin="10,0,0,0" Width="220">CORREO ELECTRÓNICO, NOMBRE DE CÓMO APARECE EN REDES SOCIALES:</TextBlock>
                                            <TextBox Margin="10,0,0,0" MinWidth="150"  ></TextBox>
                                        </StackPanel>
                                    </StackPanel>
                                </StackPanel>
                            </Border>
                        </Grid>
                    </TabItem>
                    <TabItem  Header="Control OGPI" Style="{DynamicResource TabItemStyle}">
                        <Grid>
                            <Border Background="White" CornerRadius="10"  Padding="10" Grid.ColumnSpan="2">
                                <StackPanel>

                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <StackPanel>
                                            <TextBlock Style="{DynamicResource TabItemContentTextStyle }" Margin="10,0,0,0" Width="220">FECHA DE CAPTURA:</TextBlock>
                                            <DatePicker Margin="10,0,0,0" MinWidth="150" ></DatePicker>
                                        </StackPanel>

                                        <StackPanel>
                                            <TextBlock Style="{DynamicResource TabItemContentTextStyle }" Margin="10,0,0,0" Width="220">NOMBRE DE QUIEN CAPTURÓ REGISTRO:</TextBlock>
                                            <ComboBox Margin="10,0,0,0" MinWidth="140" Height="23"></ComboBox>
                                        </StackPanel>
                                    </StackPanel>

                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <StackPanel>
                                            <TextBlock Style="{DynamicResource TabItemContentTextStyle }" Margin="10,0,0,0" >OBSERVACIONES GENERALES:</TextBlock>
                                            <TextBox Margin="10,0,0,0" MinWidth="150" Height="176" Width="997" ></TextBox>
                                        </StackPanel>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <StackPanel>
                                            <StackPanel>
                                                <TextBlock Style="{DynamicResource TabItemContentTextStyle }" Margin="10,0,0,0" >NO. TARJETA:</TextBlock>
                                                <TextBox Margin="10,0,0,0" MinWidth="150" Height="24" Width="997" />
                                            </StackPanel>
                                        </StackPanel>
                                    </StackPanel>

                                    <!-- Línea de separación horizontal -->
                                    <Separator Margin="10"/>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <StackPanel>
                                            <Button Content="Guardar" Width="719" Height="30" RenderTransformOrigin="0.506,1.34" />
                                        </StackPanel>
                                    </StackPanel>
                                </StackPanel>
                            </Border>
                        </Grid>
                    </TabItem>
                    <TabItem  Header="Señas Particulares  " 
                              Style="{DynamicResource TabItemStyle}">
